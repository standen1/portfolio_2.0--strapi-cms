{
  "collectionName": "components_job_jobs",
  "info": {
    "displayName": "Job",
    "icon": "gate"
  },
  "options": {},
  "attributes": {
    "CompanyName": {
      "type": "string",
      "required": true
    },
    "JobTitle": {
      "type": "string",
      "required": true
    },
    "StartDate": {
      "type": "date",
      "required": true
    },
    "StillEmployed": {
      "type": "boolean",
      "private": true,
      "required": true,
      "default": false
    },
    "EndDate": {
      "type": "date",
      "conditions": {
        "visible": {
          "!=": [
            {
              "var": "StillEmployed"
            },
            true
          ]
        }
      }
    },
    "WebsiteURL": {
      "type": "string"
    },
    "Responsibilities": {
      "type": "richtext",
      "required": true
    },
    "Project": {
      "type": "component",
      "component": "project.project",
      "repeatable": true
    }
  },
  "config": {}
}
